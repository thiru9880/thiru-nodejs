pipeline{
    agent{
        label "nodejs"
    }
    stages{
        stage('source code'){
            steps{
                git url:'https://github.com/thiru9880/thiru-nodejs.git', 
                    branch: 'main'
            }
        }
        stage('Build the code'){
            steps{
                sh 'npm install'
            }
        }
           
            }
        }
